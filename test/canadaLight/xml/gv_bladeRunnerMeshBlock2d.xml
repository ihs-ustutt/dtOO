<?xml version="1.0" encoding="ISO-8859-1"?>
<gv_bladeRunnerMeshBlock2d>
<!--
!
!
! mesh parameters (DOF)
!
!
-->				
  <constValue label="cV_gv_meshBlockThickness" name="sliderFloatParam" value="0.01" min="0.001" max="10.00"/>
	
<!--
!
!
! thickness distributions of mesh block around blade
!
!
-->						
	<function label="v3d2d_gv_meshBlockThickness">
    <builder  name="muParser">
			<expression value="-1.*#cV_gv_meshBlockThickness#, xx, yy"
				dimension="3"/>
      <x_one name="xx" min="0." max="1."/>
			<x_two name="yy" min="0." max="1."/>
    </builder>
  </function>	
	<function label="v3d2d_gv_meshBlockThickness_te_0">
    <builder  name="muParser">
			<expression value="0.5*#cV_gv_meshBlockThickness#, xx, yy"
				dimension="3"/>
      <x_one name="xx" min="0." max="1."/>
			<x_two name="yy" min="0." max="1."/>
    </builder>
  </function>	
	<function label="v3d2d_gv_meshBlockThickness_te_1">
    <builder  name="muParser">
			<expression value="-0.5*#cV_gv_meshBlockThickness#, xx, yy"
				dimension="3"/>
      <x_one name="xx" min="0." max="1."/>
			<x_two name="yy" min="0." max="1."/>
    </builder>
  </function>		
	
<!--
!
!
! combine thickness distribution of meshblock and blade
!
!
-->						
	<function label="vec3d2d_gv_meshBlock">
    <builder name="analyticFunction">
      <transformer 
			  name="discreteAddNormal"
        function_label="v3d2d_gv_meshBlockThickness"
				number_points_one="61"
				number_points_two="5"								
				order="3"				
				nf="v3_zz"
		  />
      <analyticFunction label="vec3d2d_gv_blade"/>
    </builder>
  </function>

<!--
!
!
! extract edges
!
!
-->							
	<function label="vec3d1d_gv_te">
    <builder name="analyticFunction">
      <analyticFunction label="~x00_f(vec3d2d_gv_blade)~"/>
    </builder>
  </function>
	<function label="vec3d1d_gv_meshBlock_te_0">
    <builder name="analyticFunction">
      <analyticFunction label="~x00_f(vec3d2d_gv_meshBlock)~"/>
    </builder>
  </function>
	<function label="vec3d1d_gv_meshBlock_te_1">
    <builder name="analyticFunction">
      <analyticFunction label="~x01_f(vec3d2d_gv_meshBlock)~"/>
    </builder>
  </function>
	
<!--
!
!
! create single surfaces at trailing edge
!
!
-->
	<replace variable="ii" values="{0}{1}">	
		<function label="vec3d2d_gv_meshBlock_te_{ii}">
			<builder name="vec3dSurfaceTwoDAFXmlBuilder">
				<analyticFunction label="vec3d1d_gv_te"/>
				<analyticFunction label="vec3d1d_gv_meshBlock_te_{ii}"/>
			</builder>
		</function>
	</replace>
	
<!--
!
!
! create combined surface at trailing edge
!
!
-->	
	<function label="vec3d2d_gv_meshBlock_te_2">
		<builder name="vec3dSurfaceTwoDAFXmlBuilder">
			<analyticFunction label="vec3d1d_gv_meshBlock_te_1"/>
			<analyticFunction label="vec3d1d_gv_te"/>
			<analyticFunction label="vec3d1d_gv_meshBlock_te_0"/>
		</builder>
	</function>

<!--
!
!
! add thickness increasing of combinded trailing edge surface
!
!
-->
	<function label="vec3d2d_gv_meshBlock_teAdd_2">
		<builder name="analyticFunction">
			<transformer 
				name="discreteAddNormal"
				function_label="v3d2d_gv_meshBlockThickness_te_0"
				number_points_one="21"
				number_points_two="5"								
				order="3"				
				nf="v3_zz"
			/>
			<analyticFunction label="vec3d2d_gv_meshBlock_te_2"/>
		</builder>
	</function>	
	
<!--
!
!
! extract edges of combined surface and reapproximate
!
!
-->		
	<replace variable="ii" values="{0.0}{0.5}{1.0}">			
		<function label="vec3d1d_gv_meshBlock_teAdd_2_{ii}">
			<builder name="analyticFunction">
				<transformer name="pickVec3dTwoDRangePercent" x_percent_two="{ii}"/>				
				<analyticFunction label="vec3d2d_gv_meshBlock_teAdd_2"/>
			</builder>
		</function>		
	</replace>
	<function label="vec3d2d_gv_meshBlock_teAdd_0">
		<builder name="vec3dSurfaceTwoDAFXmlBuilder">
			<analyticFunction label="vec3d1d_gv_meshBlock_teAdd_2_0.5"/>
			<analyticFunction label="vec3d1d_gv_meshBlock_teAdd_2_1.0"/>
		</builder>
	</function>		
	<function label="vec3d2d_gv_meshBlock_teAdd_1">
		<builder name="vec3dSurfaceTwoDAFXmlBuilder">
			<analyticFunction label="vec3d1d_gv_meshBlock_teAdd_2_0.5"/>
			<analyticFunction label="vec3d1d_gv_meshBlock_teAdd_2_0.0"/>
		</builder>
	</function>	
		
<!--
!
!
! create mesh blocks at trailing edge
!
!
-->			
	<replace variable="ii" values="{0}{1}">				
		<function label="vec3d3d_gv_meshBlock_te_{ii}">
			<builder name="vec3dTransVolThreeD">
				<analyticFunction label="vec3d2d_gv_meshBlock_te_{ii}"/>
				<analyticFunction label="vec3d2d_gv_meshBlock_teAdd_{ii}"/>
			</builder>
		</function>	
	</replace>		
		
	
