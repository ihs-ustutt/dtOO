<?xml version="1.0" encoding="ISO-8859-1"?>
<testing>
  <function label="f_lin"> 
    <builder  name="scaMuParserOneD">
      <string type="expression" value="xx"/>
      <string type="argument" value="xx"/>
      <range min="0." max=".5"/>
    </builder>
  </function>	
  <function label="f_test_1"> 
    <builder  name="vec2dCurve2dOneDFunctionDecorator" order="2">
				<Point_2 x="0.0" y="0.0"/>
				<Point_2 x="0.5" y="0.5"/>
				<Point_2 x="1.0" y="0.0"/>
    </builder>
  </function>
  <function label="f_test_2"> 
    <builder  name="vec2dCurve2dOneDFunctionDecorator" order="2">
				<Point_2 x="1.0" y="0.0"/>
				<Point_2 x="0.5" y="0.5"/>
				<Point_2 x="1.0" y="1.0"/>
    </builder>
  </function>
  <function label="f_test_3"> 
    <builder  name="vec2dCurve2dOneDFunctionDecorator" order="2">
				<Point_2 x="1.0" y="1.0"/>
				<Point_2 x="0.5" y="0.5"/>
				<Point_2 x="0.0" y="1.0"/>
    </builder>
  </function>
  <function label="f_test_4"> 
    <builder  name="vec2dCurve2dOneDFunctionDecorator" order="2">
				<Point_2 x="0.0" y="1.0"/>
				<Point_2 x="0.5" y="0.5"/>
				<Point_2 x="0.0" y="0.0"/>
    </builder>
  </function>
	
	<function label="f_test">
		<builder name="bSplineSurface2d_bSplineCurve2dFillConstructOCC">
			<analyticFunction label="f_test_1"/>
			<analyticFunction label="f_test_2"/>
			<analyticFunction label="f_test_3"/>
			<analyticFunction label="f_test_4"/>
	  </builder>
	</function>
	
  <part label="s3_0">
    <builder name="bSplineCurve_pointConstructOCC" order="2">
			<Point_3 x="0.0" y="0.0" z="0.0"/>
      <Point_3 x="30.0" y="0.0" z="50.0"/>
      <Point_3 x="100.0" y="0.0" z="50.0"/>
		</builder>
  </part>			
  <part label="s3_1">
    <builder name="bSplineCurve_pointConstructOCC" order="2">
			<Point_3 x="0.0" y="150.0" z="0.0"/>
      <Point_3 x="30.0" y="200.0" z="50.0"/>
      <Point_3 x="100.0" y="100.0" z="50.0"/>
		</builder>
  </part>			
	<part label="aS">
		<builder name="bSplineSurface_skinConstructOCC">
			<analyticGeometry label="s3_0"/>
			<analyticGeometry label="s3_1"/>
		</builder>
	</part>
	<part label="m1d_vec2dInMap2dTo3d">
		<builder name="vec2dInMap2dTo3d" percent="true">
			<analyticFunction label="f_test_1"/>
			<analyticGeometry label="aS"/>
		</builder>
	</part>
	<part label="m1d_scaInMap1dTo3d">
		<builder name="scaInMap1dTo3d" percent="true">
			<analyticFunction label="f_lin"/>
			<analyticGeometry label="m1d_vec2dInMap2dTo3d"/>
		</builder>
	</part>	
	<part label="m2d_vec2dInMap2dTo3d">
		<builder name="vec2dInMap2dTo3d" percent="true">
			<analyticFunction label="f_test"/>
			<analyticGeometry label="aS"/>
		</builder>
	</part>
</testing>
